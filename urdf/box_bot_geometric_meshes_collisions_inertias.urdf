<?xml version="1.0"?>
<robot name="box_bot">
    <material name="red">
        <color rgba="1.0 0.0 0.0 1"/>
    </material>

    <material name="green_light">
        <color rgba="0.0 1.0 0.0 1"/>
    </material>

    <material name="green_dark">
        <color rgba="0.0 0.5 0.0 1"/>
    </material>

    <material name="blue">
        <color rgba="0.0 0.0 1.0 1"/>
    </material>

    <material name="black">
        <color rgba="0.0 0.0 0.0 1"/>
    </material>

    <material name="white">
        <color rgba="1.0 1.0 1.0 1"/>
    </material>

    <link name="base_link">
    </link>

    <link name="chassis">
        <visual>
            <geometry>
                <mesh filename="package://box_bot_description/meshes/cute_cube.dae" scale="0.1 0.1 0.1"/>
            </geometry>
            <material name="blue"/>
        </visual>

        <collision>
            <geometry>
                <box size="0.1 0.1 0.1"/>
                <!-- <mesh filename="package://box_bot_description/meshes/cute_cube.dae" scale="0.1 0.1 0.1"/> -->
            </geometry>
        </collision>

        <inertial>
            <mass value="0.2"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.0003333333333333334" ixy="0" ixz="0" iyy="0.0003333333333333334" iyz="0" izz="0.0003333333333333334"/>
        </inertial>
    </link>

    <joint name="base_link_joint" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="chassis"/>
    </joint>

    <!-- Left wheel -->
    <link name="left_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder length="0.001" radius="0.035"/>
            </geometry>
            <material name="black"/>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder length="0.001" radius="0.035"/>
            </geometry>
        </collision>

        <inertial>
            <mass value="0.05"/>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <inertia ixx="1.531666666666667e-05" ixy="0" ixz="0" iyy="1.531666666666667e-05" iyz="0" izz="3.0625000000000006e-05"/>
        </inertial>
    </link>

    <joint name="left_wheel" type="continuous">
        <origin xyz="0.0 0.05 -0.025" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="left_wheel"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>

    <!-- Right wheel -->
    <link name="right_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder length="0.001" radius="0.035"/>
            </geometry>
            <material name="black"/>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder length="0.001" radius="0.035"/>
            </geometry>
        </collision>

        <inertial>
            <mass value="0.05"/>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <inertia ixx="1.531666666666667e-05" ixy="0" ixz="0" iyy="1.531666666666667e-05" iyz="0" izz="3.0625000000000006e-05"/>
        </inertial>
    </link>

    <joint name="right_wheel" type="continuous">
        <origin xyz="0.0 -0.05 -0.025" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="right_wheel"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>

    <!-- Front caster -->
    <link name="front_yaw_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder length="0.001" radius="0.0045000000000000005"/>
            </geometry>
            <material name="blue"/>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder length="0.001" radius="0.0045000000000000005"/>
            </geometry>
        </collision>

        <inertial>
            <mass="0.001"/>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <inertia ixx="5.145833333333334e-09" ixy="0" ixz="0" iyy="5.145833333333334e-09" iyz="0" izz="1.0125000000000003e-08"/>
        </inertial>
    </link>

    <joint name="front_yaw_joint" type="continuous">
        <origin xyz="0.04 0 -0.05" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="front_yaw_link"/>
        <axis xyz="0 0 1"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>

    <link name="front_roll_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder length="0.001" radius="0.0045000000000000005"/>
            </geometry>
            <material name="red"/>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder length="0.001" radius="0.0045000000000000005"/>
            </geometry>
        </collision>

        <inertial>
            <mass="0.001"/>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <inertia ixx="5.145833333333334e-09" ixy="0" ixz="0" iyy="5.145833333333334e-09" iyz="0" izz="1.0125000000000003e-08"/>
        </inertial>
    </link>

    <joint name="front_roll_joint" type="continuous">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="front_yaw_link"/>
        <child link="front_roll_link"/>
        <axis xyz="1 0 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>

    <link name="front_pitch_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.010"/>
            </geometry>
            <material name="green_dark"/>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.010"/>
            </geometry>
        </collision>

        <inertial>
            <mass value="0.001"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="4e-08" ixy="0" ixz="0" iyy="4e-08" iyz="0" izz="4e-08"/>
        </inertial>            
    </link>

    <joint name="front_pitch_joint" type="continuous">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="front_roll_link"/>
        <child link="front_pitch_link"/>
        <axis xyz="0 1 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>

    <!-- Back caster -->
    <link name="back_yaw_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder length="0.001" radius="0.0045000000000000005"/>
            </geometry>
            <material name="blue"/>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder length="0.001" radius="0.0045000000000000005"/>
            </geometry>
        </collision>

        <inertial>
            <mass="0.001"/>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <inertia ixx="5.145833333333334e-09" ixy="0" ixz="0" iyy="5.145833333333334e-09" iyz="0" izz="1.0125000000000003e-08"/>
        </inertial>
    </link>

    <joint name="back_yaw_joint" type="continuous">
        <origin xyz="-0.04 0 -0.05" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="back_yaw_link"/>
        <axis xyz="0 0 1"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>

    <link name="back_roll_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder length="0.001" radius="0.0045000000000000005"/>
            </geometry>
            <material name="red"/>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder length="0.001" radius="0.0045000000000000005"/>
            </geometry>
        </collision>

        <inertial>
            <mass="0.001"/>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <inertia ixx="5.145833333333334e-09" ixy="0" ixz="0" iyy="5.145833333333334e-09" iyz="0" izz="1.0125000000000003e-08"/>
        </inertial>
    </link>

    <joint name="back_roll_joint" type="continuous">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="back_yaw_link"/>
        <child link="back_roll_link"/>
        <axis xyz="1 0 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>

    <link name="back_pitch_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.010"/>
            </geometry>
            <material name="green_light"/>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.010"/>
            </geometry>
        </collision>

        <inertial>
            <mass value="0.001"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="4e-08" ixy="0" ixz="0" iyy="4e-08" iyz="0" izz="4e-08"/>
        </inertial>            
    </link>

    <joint name="back_pitch_joint" type="continuous">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="back_roll_link"/>
        <child link="back_pitch_link"/>
        <axis xyz="0 1 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>





</robot>